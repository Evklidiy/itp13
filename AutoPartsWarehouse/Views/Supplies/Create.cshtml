@model AutoPartsWarehouse.Models.Supply
@{ ViewData["Title"] = "Новая поставка"; }
<h1>Регистрация поставки</h1>
<form asp-action="Create">
    <div class="form-group mb-3">
        <label asp-for="SupplierId">Поставщик</label>
        <select asp-for="SupplierId" class="form-control" asp-items="ViewBag.SupplierId"></select>
    </div>
    <div class="form-group mb-3">
        <label asp-for="Date"></label>
        <input asp-for="Date" class="form-control" type="date" />
    </div>
    <div class="form-group mb-3">
        <label asp-for="Status"></label>
        <select asp-for="Status" class="form-control">
            <option>Ожидается</option>
            <option>Получена</option>
        </select>
    </div>
    <button type="submit" class="btn btn-primary">Создать (далее добавление товаров)</button>
</form>